{
  "name": "usedesk-notifications-middleware",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "start:off_ssl": "node dist/server.js --without-ssl",
    "start:dev": "nodemon src/server.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:create": "npx typeorm migration:create -c migration -n",
    "migration:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -c migration",
    "migration:revert": "npx ts-node ./node_modules/typeorm/cli.js migration:revert -c migration",
    "seed:create": "npx typeorm migration:create -c seed -n",
    "seed:run": "npx ts-node ./node_modules/typeorm/cli.js migration:run -c seed",
    "seed:revert": "npx ts-node ./node_modules/typeorm/cli.js migration:revert -c seed",
    "build": "npm i && npx tsc && npm run migrate",
    "migrate": "npm run migration:run && npm run seed:run"
  },
  "author": "humduman@gmail.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "firebase": "^8.8.1",
    "firebase-admin": "^9.11.0",
    "inversify": "^6.0.1",
    "mysql2": "^2.2.5",
    "pino": "^6.13.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.6",
    "@types/pino": "^6.3.11",
    "@types/request": "^2.48.7",
    "@types/request-promise": "^4.1.48",
    "@types/validator": "^13.6.3",
    "node-dev": "^7.0.0",
    "nodemon": "^2.0.13",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.3"
  }
}
